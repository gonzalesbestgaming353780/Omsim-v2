-- Omsim v2: Single Home Button Triggering All Features Including Auto Bounty, Aim Bot, FPS Boost, Lag Fix, Teleport Select Island, Teleport Sea 1/2/3 -- Public project for learning purposes

local Players = game:GetService("Players") local player = Players.LocalPlayer local playerGui = player:WaitForChild("PlayerGui")

-- Create ScreenGui local screenGui = Instance.new("ScreenGui") screenGui.Name = "Omsim_v2_GUI" screenGui.Parent = playerGui

-- Single button named 'Home' local button = Instance.new("TextButton") button.Name = "Home" button.Text = "Home" button.Size = UDim2.new(0, 200, 0, 50) button.Position = UDim2.new(0, 50, 0, 50) button.BackgroundColor3 = Color3.fromRGB(0, 102, 255) -- blue color button.TextColor3 = Color3.fromRGB(255, 255, 255) button.Font = Enum.Font.SourceSansBold button.TextSize = 20 button.Parent = screenGui

-- Function to teleport to a specific target local function teleportTo(targetName) local target = workspace:FindFirstChild(targetName) if target and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then player.Character.HumanoidRootPart.CFrame = target.CFrame + Vector3.new(0,5,0) print("[Safe Simulation] Teleported to "..targetName) else print("[Safe Simulation] "..targetName.." not found") end end

button.MouseButton1Click:Connect(function() -- Auto Bounty Hunt print("[Safe Simulation] Auto Bounty Hunt triggered")

-- Aim Bot
print("[Safe Simulation] Aim Bot triggered")

-- Boost FPS
settings().Rendering.QualityLevel = Enum.QualityLevel.QualityLevel01
print("[Safe Simulation] FPS Boost applied")

-- Fix Lag
print("[Safe Simulation] Lag Fix triggered")

-- Teleport Select Island (change Island1/2/3 as needed)
local selectedIsland = "Island1"
teleportTo(selectedIsland)

-- Teleport Sea 1, Sea 2, Sea 3
for i = 1, 3 do
    teleportTo("Sea"..i)
end

-- Anti-Ban
print("[Safe Simulation] Anti-Ban active")

end)

print("Omsim v2 Single Home Button GUI loaded")

